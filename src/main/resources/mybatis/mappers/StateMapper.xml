<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sustech.ooad.mapper.geoInfoMappers.StateMapper">
    <select id="getStatesByCountryCode" resultType="com.sustech.ooad.entity.geoInfo.State">
        SELECT * FROM states WHERE country_code = #{countryCode}
    </select>
</mapper>